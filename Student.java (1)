class Student{
   
    int roll;
    String name;
    
    Student(int r, String n) {
    roll=r;
    name=n;
   
    }
    void display() {
 System.out.println(roll+"\t"+name);
      
    }
    public static void main(String[] args)
      {
  
    Student s1 = new Student(101, "Vikram");
         s1.display();
          
    Student s2 = new Student(102, "Vicky"), s3 = new Student(103, "Swara"), s4 = new Student(104, "Advik");
            s2.display();
            s3.display();
            s4.display();
        }
    }
